# Development Environment Variables
DATABASE_URL=file:./dev.db
JWT_SECRET=dev-secret-key-only-for-development
PORT=9100
NODE_ENV=development

# Security & Rate Limiting
RATE_LIMIT_WINDOW=15min
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_AUTH_MAX=5
LOG_LEVEL=debug

# Redis Configuration
REDIS_ENABLED=false
REDIS_URL=redis://localhost:6379

# Sentry Configuration
SENTRY_DSN=
SENTRY_ENVIRONMENT=development

# File Upload
UPLOAD_MAX_SIZE=10485760
UPLOAD_PATH=./uploads

# CORS
CORS_ORIGINS=http://localhost:9101,http://localhost:9100,http://localhost:5173

# Email (optional)
EMAIL_ENABLED=false
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=
EMAIL_PASS=
EMAIL_FROM=noreply@truelabel.com

# Performance
ENABLE_PERFORMANCE_MONITORING=true
MEMORY_CHECK_INTERVAL=300000
CONNECTION_CLEANUP_INTERVAL=600000