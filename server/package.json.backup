{
  "name": "true-label-server",
  "version": "1.0.0",
  "description": "True Label - Backend API para Plataforma de Validação CPG",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "vercel-build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "test": "jest --runInBand",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio",
    "seed": "npx tsx prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force && npm run seed",
    "migrate:postgresql": "node scripts/migrate-to-postgresql.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest tests/integration --runInBand",
    "test:unit": "jest src --runInBand"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^9.28.1",
    "@sentry/profiling-node": "^9.28.1",
    "@types/node-cron": "^3.0.11",
    "@types/redis": "^4.0.10",
    "@types/response-time": "^2.3.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.1.0",
    "nodemailer": "^7.0.3",
    "pdf-parse": "^1.1.1",
    "prisma": "^5.6.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.3",
    "redis": "^5.5.6",
    "response-time": "^2.3.3",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
